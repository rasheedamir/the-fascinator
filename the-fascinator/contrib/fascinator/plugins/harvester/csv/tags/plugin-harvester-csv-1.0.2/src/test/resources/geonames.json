{
    "harvester": {
        "type": "csv",
        "csv": {
            "fileLocation": "${test.dir}/geonames.csv",
            "idColumn": "geonameid",
            "recordIDPrefix": "http://geonames.org/",
            "headerRow": false,
            "headerList": ["geonameid", 
                            "name",
                            "asciiname",
                            "alternatenames",
                            "latitude",
                            "longitude",
                            "feature class",
                            "feature code",
                            "country code",
                            "cc2",
                            "admin1 code",
                            "admin2 code",
                            "admin3 code",
                            "admin4 code",
                            "population",
                            "elevation",
                            "gtopo30",
                            "timezone",
                            "modification date"],
            "delimiter": "\t",
            "includedFields" : ["geonameid", 
                                "name",
                                "asciiname",
                                "alternatenames",
                                "latitude",
                                "longitude",
                                "feature class",
                                "feature code",
                                "country code",
                                "cc2", 
                                "admin1 code", 
                                "admin2 code",
                                "admin3 code", 
                                "admin4 code", 
                                "population", 
                                "elevation", 
                                "gtopo30", 
                                "timezone", 
                                "modification date"]
        }
    }
}
